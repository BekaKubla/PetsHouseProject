@model PetsProject.ViewModels.DamakebaViewModel
<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Colorlib Templates">
    <meta name="author" content="Colorlib">
    <meta name="keywords" content="Colorlib Templates">

    <!-- Title Page-->
    <title>Au Register Forms by Colorlib</title>

    <!-- Icons font CSS-->
    <link href="~/PetRegistration/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">
    <link href="~/PetRegistration/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <!-- Font special for pages-->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i" rel="stylesheet">

    <!-- Vendor CSS-->
    <link href="~/PetRegistration/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="~/PetRegistration/vendor/datepicker/daterangepicker.css" rel="stylesheet" media="all">

    <!-- Main CSS-->
    <link href="~/PetRegistration/css/main.css" rel="stylesheet" media="all">
</head>

<body>
    <div class="container">
        <h1 class="well">დამაკება</h1>
        <div class="col-lg-12 well">
            <div class="row">
                <form method="post" autocomplete="off" enctype="multipart/form-data" asp-controller="Damakeba" asp-action="Edit">
                    <div class="col-sm-12">
                        <div class="row">
                            <div class="col-sm-6 form-group">
                                <label>სათაური</label>
                                <input type="text" asp-for="Title" autocomplete="off" placeholder="ჩაწერეთ სათაური.." class="form-control">
                                <span asp-validation-for="Title" class="text-danger"></span>
                            </div>
                            <div class="col-sm-6 form-group">
                                <label>სქესი</label>
                                <select asp-for="Sex" class="form-control" asp-items="@Html.GetEnumSelectList<PetsProject.Models.Sex>()">
                                    <option value="">გთხოვთ აირჩიეთ სქესი..</option>
                                </select>
                                <span asp-validation-for="Sex" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-4 form-group">
                                <label>ჯიში</label>
                                <select asp-for="Jishebi" class="form-control" asp-items="@Html.GetEnumSelectList<PetsProject.Models.Jishebi>()">
                                    <option value="">გთხოვთ აირჩიეთ ჯიში..</option>
                                </select>
                                <span asp-validation-for="Jishebi" class="text-danger"></span>
                            </div>
                            <div class="col-sm-4 form-group">
                                <label>ქალაქი</label>
                                <select asp-for="City" class="form-control" asp-items="@Html.GetEnumSelectList<PetsProject.Models.City>()">
                                    <option value="">გთხოვთ აირჩიეთ ჯიში..</option>
                                </select>
                                <span asp-validation-for="City" class="text-danger"></span>
                            </div>
                            <div class="col-sm-4 form-group">
                                <label>ძაღლის ასაკი</label>
                                <input type="text" placeholder="ჩაწერეთ ძაღლის ასაკი.." autocomplete="off" asp-for="Age" class="form-control">
                                <span asp-validation-for="Age" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12 form-group">
                                <label>აღწერა</label>
                                <textarea placeholder="დაწერეთ თქვენს შესახებ(მაქს.300 სიტყვა).." asp-for="Description" rows="3" class="form-control"></textarea>
                                <span asp-validation-for="Description" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-6 form-group">
                                <label>თქვენი სახელი</label>
                                <input type="text" placeholder="ჩაწერეთ თქვენი სახელი.." autocomplete="off" asp-for="EmployeName" class="form-control">
                                <span asp-validation-for="EmployeName" class="text-danger"></span>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group mb-4">
                                    <label>საბუთი</label>
                                    <select asp-for="Document" class="form-control" asp-items="@Html.GetEnumSelectList<PetsProject.Models.Document>()">
                                        <option disabled="disabled" value="">გთხოვთ აირჩიეთ სქესი..</option>
                                    </select>
                                    <span asp-validation-for="Document" class="text-danger"></span>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label>მობილურის ნომერი</label>
                            <input type="text" placeholder=" xxx xx xx xx.." asp-for="PhoneNumber" autocomplete="off" class="form-control">
                            <span asp-validation-for="PhoneNumber" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label>ატვირთე ფოტო (ატვირთეთ თუ არსებობს ფოტო)</label>
                            <div class="custom-file">
                                <input asp-for="DamakebaPhoto" class="custom-file-input" />
                                <label class="custom-file-label" for="customFile">ფოტოს ატვირთვა...</label>
                                <span asp-validation-for="DamakebaPhoto" class="text-danger">ფოტოს ზომა : 3x4</span>
                            </div>
                        </div>
                        <button type="submit" class="btn btn-lg btn-info">განცხადების განახლება</button>
                    </div>
                </form>
            </div>
        </div>
    </div>

    <!-- Jquery JS-->
    <script src="~/PetRegistration/vendor/jquery/jquery.min.js"></script>
    <!-- Vendor JS-->
    <script src="~/PetRegistration/vendor/select2/select2.min.js"></script>
    <script src="~/PetRegistration/vendor/datepicker/moment.min.js"></script>
    <script src="~/PetRegistration/vendor/datepicker/daterangepicker.js"></script>

    <!-- Main JS-->
    <script src="~/PetRegistration/js/global.js"></script>

</body>

</html>




<!-- loader -->
<div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>


<script src="~/Main/js/jquery.min.js"></script>
<script src="~/Main/js/jquery-migrate-3.0.1.min.js"></script>
<script src="~/Main/js/popper.min.js"></script>
<script src="~/Main/js/bootstrap.min.js"></script>
<script src="~/Main/js/jquery.easing.1.3.js"></script>
<script src="~/Main/js/jquery.waypoints.min.js"></script>
<script src="~/Main/js/jquery.stellar.min.js"></script>
<script src="~/Main/js/jquery.animateNumber.min.js"></script>
<script src="~/Main/js/bootstrap-datepicker.js"></script>
<script src="~/Main/js/jquery.timepicker.min.js"></script>
<script src="~/Main/js/owl.carousel.min.js"></script>
<script src="~/Main/js/jquery.magnific-popup.min.js"></script>
<script src="~/Main/js/scrollax.min.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false"></script>
<script src="~/Main/js/google-map.js"></script>
<script src="~/Main/js/main.js"></script>